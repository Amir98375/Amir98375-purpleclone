let Top_Pick = JSON.parse(localStorage.getItem("TOPPICKS"));
 toppickproducts ( Top_Pick);

function toppickproducts ( Top_Pick){
     Top_Pick.map(function (el) {
      let div = document.createElement("div");
      div.setAttribute("id","divjust")

      let image = document.createElement("img");
      image.src = el.img;
      image.setAttribute("id","img12")

      let name = document.createElement("p");
      name.innerHTML= el.name;
      name.setAttribute("id","name")

      let price = document.createElement("p");
      price.innerHTML = `₹${el.price}`;
      price.setAttribute("id","price");

      div.append(image,name,price);

      document.getElementById("toppicks").append(div);
    }) 
}

